<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <include
        android:id="@+id/include_gold_and_gem_weight_gm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_gold_and_gem_weight_gm1"
        app:layout_constraintTop_toTopOf="parent"/>

    <include
        android:id="@+id/include_gold_and_gem_weight_kpy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_gold_and_gem_weight_kpy1"
        app:layout_constraintTop_toBottomOf="@id/include_gold_and_gem_weight_gm"/>

    <include
        android:id="@+id/include_gem_weight"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_gem_weight1"
        app:layout_constraintTop_toBottomOf="@id/include_gold_and_gem_weight_kpy"
        />

    <include
        android:id="@+id/include_gee_weight"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_gee_weight1"
        app:layout_constraintTop_toBottomOf="@id/include_gem_weight"/>

    <include
        android:id="@+id/include_item_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_item_type"
        app:layout_constraintTop_toBottomOf="@id/include_gee_weight"/>

    <include
        android:id="@+id/include_goldQ"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_gold_q1"
        app:layout_constraintTop_toBottomOf="@id/include_item_type"/>

    <include
        android:id="@+id/include_general_expense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_constraint_general_expense"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/include_goldQ"/>

    <include
        android:id="@+id/include_purchase_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_purchase_price1"
        app:layout_constraintTop_toBottomOf="@id/include_general_expense"/>

    <include
        android:id="@+id/include_mortgage_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_mortgage_price1"
        app:layout_constraintTop_toBottomOf="@id/include_purchase_price"/>

    <include
        android:id="@+id/include_voucher_open_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_voucher_open_pay1"
        app:layout_constraintTop_toBottomOf="@id/include_mortgage_price"/>

    <include
        android:id="@+id/include_voucher_open_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_voucher_open_price1"
        app:layout_constraintTop_toBottomOf="@id/include_voucher_open_pay"/>

    <include
        android:id="@+id/include_purchase_gold_weight"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_purchase_gold_weight1"
        app:layout_constraintTop_toBottomOf="@id/include_voucher_open_price"/>



</androidx.constraintlayout.widget.ConstraintLayout>