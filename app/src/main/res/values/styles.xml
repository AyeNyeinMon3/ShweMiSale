<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LoginHintText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/primary</item>
    </style>

    <style name="itemText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="chipText" parent="TextAppearance.AppCompat">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="materialCard_x"  parent="@style/Widget.MaterialComponents.CardView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>


    </style>

    <style name="materialCard_y"  parent="@style/Widget.MaterialComponents.CardView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>

    </style>


    <style name="materialCard_z"  parent="@style/Widget.MaterialComponents.CardView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeTopRight">10dp</item>
        <item name="cornerSizeBottomRight">10dp</item>

    </style>

</resources>